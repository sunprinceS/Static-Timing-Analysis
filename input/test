module mul2 ( e, f, g, h, a, b, c,d );
    input a, b, c, d;
	 output e, f, g, h;
    wire n1, n2, n3, n5, n6, n7, n8, n9, n10, n11, n12, n13, n14;

    NOT1 U1 ( .A(n13), .Y(n1) );
    NOT1 U2 ( .A(n11), .Y(n2) );
    NOT1 U3 ( .A(n9), .Y(n3) );
    NOT1 U4 ( .A(n14), .Y(e) );
    NOT1 U5 ( .A(d), .Y(n5) );
    NOR2 U6 ( .A(n6), .B(n7), .Y(h) );
    NAND2 U7 ( .A(d), .B(c), .Y(n7) );
    NAND2 U8 ( .A(b), .B(a), .Y(n6) );
    NOR2 U9 ( .A(n5), .B(n8), .Y(g) );
    NAND2 U10 ( .A(b), .B(n3), .Y(n8) );
    NOR2 U11 ( .A(n10), .B(n11), .Y(n9) );
    NAND2 U12 ( .A(n12), .B(n1), .Y(f) );
    NOR2 U13 ( .A(n10), .B(n2), .Y(n13) );
    NAND2 U14 ( .A(n2), .B(n10), .Y(n12) );
    NAND2 U15 ( .A(d), .B(a), .Y(n10) );
    NAND2 U16 ( .A(c), .B(b), .Y(n11) );
    NAND2 U17 ( .A(a), .B(c), .Y(n14) );
endmodule



